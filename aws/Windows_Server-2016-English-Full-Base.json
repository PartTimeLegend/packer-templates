{    
    "builders": [
        {
            "type": "amazon-ebs",
            "profile" : "{{ user `aws_profile` }}",
            "name": "{{ user `Windows_Server-2016-English-Full-Base_ami_name` }}",
            "ami_name": "{{ user `Windows_Server-2016-English-Full-Base_ami_name` }}",
            "region": "{{ user `aws_region` }}",
            "instance_type": "{{ user `Windows_Server-2016-English-Full-Base_aws_instance_type` }}",
            "source_ami": "{{user `Windows_Server-2016-English-Full-Base_aws_source_ami`}}",
            "vpc_id": "{{ user `aws_build_vpc` }}",
            "user_data_file": "userdata/userdata.txt",
            "communicator": "winrm",
            "winrm_timeout": "20m",
            "winrm_use_ssl": false,
            "winrm_insecure": true,
            "winrm_username": "Administrator",
	        "winrm_password": "{{ user `winrm_password` }}",
	        "winrm_use_ntlm": true,
            "subnet_id": "{{ user `aws_build_subnet` }}",
            "associate_public_ip_address": "{{user `aws_public_ip`}}",
            "ssh_keypair_name" : "{{user `aws_keypair_name`}}",
            "ssh_private_key_file": "{{user `aws_private_key_file`}}",
            "iam_instance_profile": "{{user `aws_iam_instance_profile`}}",
            "run_tags": {
                "Name": "packer-builder-{{ user `Windows_Server-2016-English-Full-Base_ami_name` }}",
                "builder": "packer",
                "UUID": "{{ user `unique_id`}}",
                "Instance_Stop": "#23:45 UTC weekend",
                "Instance_Start": "#09:00 UTC weekday",
                "CreatedBy": "Antony Bailey"
            },
            "tags": {
                "Name": "{{ user `Windows_Server-2016-English-Full-Base_ami_name` }}",
                "UUID": "{{ user `unique_id`}}",
                "builder": "packer",
                "source_ami": "{{user `Windows_Server-2016-English-Full-Base_aws_source_ami`}}",
                "CreatedBy": "Antony Bailey"
            }
        }
    ],
    "provisioners": [
        {
            "type": "powershell",
            "inline": [
              "C:/ProgramData/Amazon/EC2-Windows/Launch/Scripts/InitializeInstance.ps1 -Schedule",
              "C:/ProgramData/Amazon/EC2-Windows/Launch/Scripts/SysprepInstance.ps1 -NoShutdown"
            ]
        }
    ]
}